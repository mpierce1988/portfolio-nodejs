<article>
    <h3 align="center">{{projectName}}</h3>    
    <div style="display: flex; justify-content:center;">
        <img src="{{linkToProjectImage}}" alt="">
    </div> 
    <br>
    
    <p align="center">{{projectDescription}}</p>
    <h3 class="primary">Features:</h3>
    <ul>
        {{#each features}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    <a href='{{linkToProject}}'><input type="button" value="Click Here!"></a>
</article>