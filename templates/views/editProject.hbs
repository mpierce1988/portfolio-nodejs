
<form action="/admin/projects/{{project.id}}" method="post">
    {{#if message}}
        <h3>{{message}}</h3>
    {{/if}}

    {{#if error}}
        <h3>{{error}}</h3>
    {{/if}}
    <input type="hidden" name="_method" value="patch">
    {{>projectEditable project}}
    <button id="save">Save Changes</button>
</form>